{{- /*
  constants 常量：集中管理命名模板所需的常量定义
  注：有一部分正则表达式定义分布在各个命名模板中，仅适用于该命名模板内部
*/ -}}
{{- define "base.env" -}}
# 默认空字符
emptyStr: ""
# 检查字符串是否为 float64 / int / int64 类型
regexCheckInt: "^[+-]?\\d+(\\.\\d+)?$"
# 清洗全数字的 string
regexReplaceZero: "^([+-]?)0+(\\d*)$"
# 检查八进制和十六进制字符串
regexOctalHex: "^0(x|X|o|O)"
# 分割字符串时的分隔符 " "、","、":"、"."、"|"、"/"、"^"、"@"、"#"、"*"
regexSplitStr: "\\s*[,\\:\\.\\|\\/\\*\\^@#\\s]+\\s*"
# 默认字符串分隔符(空格)
# 用于处理一些需要保留特殊字符的情况
regexSplit: "\\s+"
# 字符串分隔符(逗号)
# 用于处理一些需要保留特殊字符的情况
regexSplitComma: ",\\s*"

regexRFC1035: "^([a-z]{1,63}|[a-z][a-z0-9-]{0,61}[a-z0-9]?)$"
regexRFC1123: "^([a-z0-9]{1,63}|[a-z0-9][a-z0-9-]{0,61}[a-z0-9]?)$"
regexQuantity: "^[+-]?(\\d+\\.?\\d{0,3}|\\.\\d{1,3})([KMGTPE]i|[mkMGTPE]|[eE]\\s?[+-]?(\\d+\\.?\\d{0,3}|\\.\\d{1,3}))?$"
regexResources: "^([+-]?(\\d+\\.?\\d{0,3}|\\.\\d{1,3})([KMGTPE]i|[mkMGTPE]|[eE]\\s?[+-]?(\\d+\\.?\\d{0,3}|\\.\\d{1,3}))?\\s*){1,4}$"
regexTime: "^[-+]?(\\d+(\\.\\d+)?(ns|us|µs|ms|s|m|h))+$"
regexFieldsV1: "^(\\.|f\\:[^\\:]+|i\\:\\d+|v\\:.+|k\\:.+)$"
regexIP: "^((0|25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]?)\\.){3}(0|25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]?)(\\/\\d{1,2})?$"
regexFileMode: "^(0[0-7]{3}|0|[1-9]\\d|[1-4]\\d{2}|50\\d|51[0-1])$"
regexPercent: "^\\d+(\\%)?$"
regexPort: "^((0)|([1-9]\\d{0,3})|([1-5]\\d{4})|(6[0-4]\\d{3})|(65[0-4]\\d{2})|(655[0-2]\\d)|(6553[0-5]))$"
regexCron: "^((\\*|[0-5]?\\d(\\-[0-5]?\\d)?)(\\/[0-5]?\\d)?(\\,[0-5]?\\d(\\-[0-5]?\\d)?(\\/[0-5]?\\d)?)*)\\s+((\\*|([0-1]?\\d|2[0-3])(\\-([0-1]?\\d|2[0-3]))?)(\\/([0-1]?\\d|2[0-3]))?(\\,([0-1]?\\d|2[0-3])(\\-([0-1]?\\d|2[0-3]))?(\\/([0-1]?\\d|2[0-3]))?)*)\\s+((\\*|([0-2]?\\d|3[01])(\\-([0-2]?\\d|3[01]))?)(\\/([0-2]?\\d|3[01]))?(\\,([0-2]?\\d|3[01])(\\-([0-2]?\\d|3[01]))?(\\/([0-2]?\\d|3[01]))?)*)\\s+((\\*|(0?[1-9]|1[0-2]|JAN|jan|FEB|feb|MAR|mar|APR|apr|MAY|may|JUN|jun|JUL|jul|AUG|aug|SEP|sep|OCT|oct|NOV|nov|DEC|dec)(\\-(0?[1-9]|1[0-2]|JAN|jan|FEB|feb|MAR|mar|APR|apr|MAY|may|JUN|jun|JUL|jul|AUG|aug|SEP|sep|OCT|oct|NOV|nov|DEC|dec))?)(\\/(0?[1-9]|1[0-2]|JAN|jan|FEB|feb|MAR|mar|APR|apr|MAY|may|JUN|jun|JUL|jul|AUG|aug|SEP|sep|OCT|oct|NOV|nov|DEC|dec))?(\\,(0?[1-9]|1[0-2]|JAN|jan|FEB|feb|MAR|mar|APR|apr|MAY|may|JUN|jun|JUL|jul|AUG|aug|SEP|sep|OCT|oct|NOV|nov|DEC|dec)(\\-(0?[1-9]|1[0-2]|JAN|jan|FEB|feb|MAR|mar|APR|apr|MAY|may|JUN|jun|JUL|jul|AUG|aug|SEP|sep|OCT|oct|NOV|nov|DEC|dec))?(\\/(0?[1-9]|1[0-2]|JAN|jan|FEB|feb|MAR|mar|APR|apr|MAY|may|JUN|jun|JUL|jul|AUG|aug|SEP|sep|OCT|oct|NOV|nov|DEC|dec))?)*)\\s+((\\*|(0?[0-7]|SUN|sun|MON|mon|TUE|tue|WED|wed|THU|thu|FRI|fri|SAT|sat)(\\-(0?[0-7]|SUN|sun|MON|mon|TUE|tue|WED|wed|THU|thu|FRI|fri|SAT|sat))?)(\\/(0?[0-7]|SUN|sun|MON|mon|TUE|tue|WED|wed|THU|thu|FRI|fri|SAT|sat))?(\\,(0?[0-7]|SUN|sun|MON|mon|TUE|tue|WED|wed|THU|thu|FRI|fri|SAT|sat)(\\-(0?[0-7]|SUN|sun|MON|mon|TUE|tue|WED|wed|THU|thu|FRI|fri|SAT|sat))?(\\/(0?[0-7]|SUN|sun|MON|mon|TUE|tue|WED|wed|THU|thu|FRI|fri|SAT|sat))?)*)$"

regexConfigMap: "^configMap\\s+(\\S+)(?:\\s+(\\S+))?(?:\\s+(true|false))?$"
regexContainerPort: "^((?:((?:\\d{1,3}\\.){3}\\d{1,3}):)?(?:(\\d{1,5}):)?)?(\\d{1,5})(?:/(TCP|tcp|UDP|udp|SCTP|sctp))?(?:#(\\S+))?$"
regexContainerResizePolicy: "^(cpu|memory)\\s*(NotRequired|RestartContainer)?$"
regexContainerRestartRule: "^((?i)Restart)\\s+(in|notin)\\s+\\((.*?)\\)$"
regexUpdateStrategy: "^(OnDelete|RollingUpdate)?(?:\\s+(\\d+\\%?)){0,2}$"
regexStrategy: "^(Recreate|RollingUpdate)(?:\\s+(\\d+\\%?)){0,2}$"
regexEnvFromSource: "^(configMap|secret)\\s+(\\S+)(?:\\s+(\\S+))?(?:\\s+(true|false))?$"
regexEnvVarSource: "^(configMap|field|file|resource|secret)\\s+(\\S+)(?:\\s+(\\S+))?(?:\\s+(\\S+))?(?:\\s+(\\S+))?$"
regexFileKeySelector: "^file\\s+(\\S+)\\s+(\\S+)\\s+(\\S+)(?:\\s+(true|false))?$"
regexGRPCAction: "^(?:(\\S+)\\s+)?(\\d+)$"
regexHostAlias: "^((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})(\\.((2(5[0-5]|[0-4]\\d))|[0-1]?\\d{1,2})){3}(\\s+[a-zA-Z0-9][-a-zA-Z0-9]{0,62}(\\.[a-zA-Z0-9][-a-zA-Z0-9]{0,62})+)+$"
regexHTTPGetAction: "^(?:(HTTP|http|HTTPS|https)\\s+)?(?:(\\S+)\\s+)?(?:(\\d+)\\s+)?(\\S+)(?:\\s+(header|headers)\\s+\\((.*?)\\))?$"
regexHTTPIngressPathResource: "^resource(?:\\s+(\\S+))(?:\\s+(\\S+))(?:\\s+(\\S+))?$"
regexHTTPIngressPathService: "^service(?:\\s+(\\S+))(?:\\s+([\\w-]+|0|[1-9]\\d{0,3}|[1-5]\\d{4}|6[0-4]\\d{3}|65[0-4]\\d{2}|655[0-2]\\d|6553[0-5]))$"
regexHTTPIngressRuleValue: "^(\\S+)(?:\\s+(Exact|Prefix|ImplementationSpecific))?(?:\\s+(resource|service)(?:\\s+(.*?)))$"
regexIngressBackendResource: "^resource(?:\\s+(\\S+))(?:\\s+(\\S+))(?:\\s+(\\S+))?$"
regexIngressBackendService: "^service(?:\\s+(\\S+))(?:\\s+([\\w-]+|0|[1-9]\\d{0,3}|[1-5]\\d{4}|6[0-4]\\d{3}|65[0-4]\\d{2}|655[0-2]\\d|6553[0-5]))$"
regexIngressSpecResource: "^resource(?:\\s+(\\S+))(?:\\s+(\\S+))(?:\\s+(\\S+))?$"
regexIngressSpecService: "^service(?:\\s+(\\S+))(?:\\s+([\\w-]+|0|[1-9]\\d{0,3}|[1-5]\\d{4}|6[0-4]\\d{3}|65[0-4]\\d{2}|655[0-2]\\d|6553[0-5]))$"
regexIngressSpecTLS: "^([a-z0-9-]+)(?:\\s+((?:\\*\\.)?[a-z0-9-]+(?:\\.[a-z0-9-]+)+))(?:\\s+((?:\\*\\.)?[a-z0-9-]+(?:\\.[a-z0-9-]+)+))*$"
regexEquality: "^[A-Za-z0-9-]+\\s+([=]{1,2}|!=)\\s+[A-Za-z0-9-]+$"
regexEquality1: "^[A-Za-z0-9-]+\\s+(([=]{1,2}|!=)\\s+[A-Za-z0-9-]+|[><]\\s+\\+?\\d+)$"
regexSet: "^[A-Za-z0-9-]+\\s+([iI][nN]|[nN][oO][tT][iI][nN])\\s+\\(([A-Za-z0-9-]+(\\s+|\\s*,\\s*)*)+\\)$"
regexSet1: "^[A-Za-z0-9-]+\\s+(([iI][nN]|[nN][oO][tT][iI][nN])\\s+\\(([A-Za-z0-9-]+(\\s+|\\s*,\\s*)*)+\\)|([gG][tT]|[lL][tT])\\s+\\d+)$"
regexSetExists: "^!?[A-Za-z0-9-]+"
regexReservedNamespace: "^(default|kube-.*)$"
regexObjectFieldSelector: "^field\\s+(\\S+)(?:\\s+(\\S+))?$"
regexPersistentVolumeClaim: "^(\\S+)\\s+(\\S+)(?:\\s+(\\S+))?\\s+(accessModes?)\\s*\\(\\s*([^)]+?)\\s*\\)\\s+(\\S+)(?:\\s+(\\S+))?(?:\\s+(\\S+))?(?:\\s+(Filesystem|filesystem|Block|block))?$"
regexPodFailurePolicyOnExitCodesRequirement: "^(FailJob|FailIndex|Ignore|Count)(?:\\s+(\\S+))?(?:\\s+(in|notin))(?:\\s+\\((.*?)\\))$"
regexPodFailurePolicyOnExitCodesRequirementValues: "^\\d+(,\\s*\\d+)*$"
regexPodFailurePolicyOnPodConditionsPattern: "^(\\S+)(?:\\s+(\\S+))?$"
regexPodFailurePolicyRule1: "^(FailJob|FailIndex|Ignore|Count)(?:\\s+(\\S+))?(?:\\s+(in|notin))(?:\\s+\\((.*?)\\))$"
regexPodFailurePolicyRule2: "^(FailJob|FailIndex|Ignore|Count)(?:\\s+\\((.*?)\\))$"
regexVolumes: "^(cm|configMap|secret|pvc|persistentVolumeClaim|emptyDir|hostPath|nfs|image|fc|iscsi|local)(?:\\s+(.*?))(?:\\s+(.*?))?$"
regexVolumeConfigMap: "^([a-z]\\w+)\\s*(true|false)?\\s*(\\d+)?\\s*(?:items\\s*\\((.*?)\\))?$"
regexVolumeEmptyDir: "^((?i)Memory)?(?:\\s*((?:\\d+(?:\\.\\d{0,3})?|\\.\\d{1,3})(?:[KMGTPE]i|[mkMGTPE])?))?$"
regexVolumeFC1: "^targetWWNs\\s*\\((.*?)\\)\\s+(\\d+)\\s+(ext4|xfs|ntfs)\\s*(true|false)?$"
regexVolumeFC2: "^wwids\\s*\\((.*?)\\)\\s+(ext4|xfs|ntfs)\\s*(true|false)?$"
regexVolumeHostPath: "^(\\S+)\\s*(DirectoryOrCreate|Directory|FileOrCreate|File|Socket|CharDevice|BlockDevice)?$"
regexVolumeImage: "^(\\S+)(?:\\s+(Always|Never|IfNotPresent))?$"
regexVolumeIscsi: "^(\\S+)\\s+(\\S+)\\s+(\\d+)\\s+(ext4|xfs|ntfs)(?:\\s+(true|false))(?:\\s+(\\S+))?(?:\\s+(\\S+))(?:\\s+(?:chap\\s*\\((true|false),\\s*(true|false)\\)\\s*))?(?:\\s+(?:portals\\s*\\((.*?)\\)\\s*))?(?:\\s+(\\S+)\\s*)?$"
regexVolumeNFS: "^(\\S+)(?:\\s+(\\S+))(?:\\s+(true|false))?$"
regexVolumePersistentVolumeClaim: "^(\\S+)(?:\\s+(true|false))?$"
regexVolumeSecret: "^([a-z]\\w+)(?:\\s+(true|false))?(?:\\s+(\\d+))?(?:\\s+(?:items\\s*\\((.*?)\\)))?$"
regexVolumeLocal: "^(\\S+)(?:\\s+(ext4|xfs|ntfs))?$"
regexResourceFieldSelector: "^resource\\s+(\\S+)(?:\\s+(\\S+))?(?:\\s+(\\S+))?$"
regexSecretEnvSource: "^secret\\s+(\\S+)(?:\\s+(\\S+))?(?:\\s+(true|false))?$"
regexSecretKeySelector: "^secret\\s+(\\S+)\\s+(\\S+)(?:\\s+(true|false))?$"
regexServiceSpecPorts: "^(?:(3(?:[01]\\d{3}|2[0-6]\\d{2}|27[0-5]\\d|276[0-7]))\\:)?(0|[1-9]\\d{0,3}|[1-5]\\d{4}|6[0-4]\\d{3}|65[0-4]\\d{2}|655[0-2]\\d|6553[0-5])(?:\\:(0|[1-9]\\d{0,3}|[1-5]\\d{4}|6[0-4]\\d{3}|65[0-4]\\d{2}|655[0-2]\\d|6553[0-5]|[\\w-]+))?(?:\\/(tcp|TCP|udp|UDP|sctp|SCTP))?(?:@([\\w\\.-\\/]+))?(?:#([\\w-]+))?$"
regexStatefulSetPersistentVolumeClaimRetentionPolicy: "^(Retain|retain|Delete|delete)?(?:\\s+(Retain|retain|Delete|delete))?$"
regexStatefulSetUpdateStrategy: "^(OnDelete|RollingUpdate)?(?:\\s*(\\d+\\%?))?(?:\\s*(\\d+))?$"
regexSuccessPolicyRule: "^(\\d+)?(?:\\s*((?:\\d+(?:-\\d+)?)(?:,\\s*\\d+(?:-\\d+)?)*))?$"
regexVolumeDevice: "^(\\S+)\\s+(\\S+)$"
regexVolumeMount: "^(\\S+)\\s+(\\S+)\\s*(\\S+)?\\s*(\\S+)?\\s*(true|false)?\\s*(Disabled|IfPossible|Enabled)?\\s*(Bidirectional|HostToContainer|None)?$"
{{- end }}
